<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.mappers.SixOneMapper">
	<!-- 49选7 类型 -->
	<resultMap type="SixOne" id="sixone">
		<id property="id" column="id" />
		<result property="qishu" column="qishu" />
		<result property="frist" column="frist" />
		<result property="second" column="second" />
		<result property="third" column="third" />
		<result property="fourth" column="fourth" />
		<result property="fifth" column="fifth" />
		<result property="sixth" column="sixth" />
		<result property="seventh" column="seventh" />
	</resultMap>

	<!-- 根据对象的属性值查找符合条件的 对象 -->
	<select id="findSixOnes" parameterType="SixOne" resultType="SixOne">
		select * from tb_caipiao_six_one
		<trim suffixOverrides="and">
			<where>
				<if test="id!=null">id=#{id} and</if>
				<if test="qishu!=null">qishu=#{qishu} and</if>
				<if test="frist!=null">first=#{first} and</if>
				<if test="second!=null">second=#{second} and</if>
				<if test="third!=null">third=#{third} and</if>
				<if test="fourth!=null">fourth=#{fourth} and</if>
				<if test="fifth!=null">fifth=#{fifth} and</if>
				<if test="sixth!=null">sixth=#{sixth} and</if>
				<if test="seventh!=null">seventh=#{seventh} and</if>
			</where>
		</trim>

	</select>
	<!-- 查询所有 升序-->
	<select id="findAllSixOnes" resultType="SixOne">
		select * from tb_caipiao_six_one order by tb_caipiao_six_one.qishu asc
	</select>

	<select id="queryTopCatalogList" parameterType="Catalog"
		resultType="Catalog">
		SELECT top ${topNum} * FROM T_CATALOG
		<trim suffixOverrides="and">
			<where>
				<if test="theID!=null">theID=#{theID} and</if>
				<if test="parentID!=null">parentID=#{parentID} and</if>
				<if test="title!=null">title=#{title} and</if>
				<if test="status!=null">status=#{status} and</if>
				<if test="sort!=null">sort=#{sort} and</if>
				<if test="catalogType!=null">catalogType=#{catalogType} and</if>
				<if test="url!=null">url=#{url} and</if>
				<if test="isFirstCatalog!=null">isFirstCatalog=#{isFirstCatalog} and</if>
				<if test="isUploadPic!=null">isUploadPic=#{isUploadPic} and</if>
			</where>
		</trim>
		order by sort asc
	</select>

	<select id="queryCatalogByParentID" parameterType="string"
		resultType="Catalog">
		select * from t_catalog where parentID = #{value}
	</select>
</mapper>
